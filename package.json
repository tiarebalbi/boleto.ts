{
  "name": "@tiarebalbi/boleto.ts",
  "version": "0.1.0",
  "description": "A TypeScript library for parsing and rendering Brazilian bank payment slips (boletos banc√°rios)",
  "type": "module",
  "license": "MIT",
  "author": "Tiare Balbi <me@tiarebalbi.com> (https://github.com/tiarebalbi)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tiarebalbi/boleto.ts.git"
  },
  "bugs": {
    "url": "https://github.com/tiarebalbi/boleto.ts/issues"
  },
  "homepage": "https://github.com/tiarebalbi/boleto.ts#readme",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "keywords": [
    "boleto",
    "boleto-bancario",
    "brazil",
    "brazilian",
    "barcode",
    "payment",
    "banking",
    "typescript"
  ],
  "main": "./dist/boleto.cjs",
  "module": "./dist/boleto.js",
  "types": "./dist/boleto.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/boleto.d.ts",
        "default": "./dist/boleto.js"
      },
      "require": {
        "types": "./dist/boleto.d.ts",
        "default": "./dist/boleto.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write \"**/*.{ts,js,jsx,tsx,json,md}\" \"eslint.config.js\" \"vite.config.ts\"",
    "format:check": "prettier --check \"**/*.{ts,js,jsx,tsx,json,md}\" \"eslint.config.js\" \"vite.config.ts\"",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.7.4",
    "jsdom": "^27.4.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.51.0",
    "vite": "npm:rolldown-vite@7.2.5",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.16"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.2.5"
  }
}
